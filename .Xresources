Xcursor.theme: DMZ-White

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.font: xft:inconsolata:size=15
URxvt.boldFont: xft:inconsolata:size=15:bold
URxvt.italicFont: xft:incosolata:size=15:italic
URxvt.boldItalicFont: xft:inconsolata:size=15:bold:italic

URxvt.letterSpace:  -1      

URxvt.perl-ext-common: default,resize-font,tabbedex

*background: #232C31
*foreground: #C5C8C6
! black
*color0: #2D3C46
*color8: #425059
! red
*color1: #A54242
*color9: #CC6666
! green
*color2: #8C9440
*color10: #B5BD68
! yellow
*color3: #DE935F
*color11: #F0C674
! blue
*color4: #5F819D
*color12: #81A2BE
! magenta
*color5: #85678F
*color13: #B294BB
! cyan
*color6: #5E8D87
*color14: #8ABEB7
! white
*color7: #6C7A80
*color15: #C5C8C6

URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false

URxvt*fading:              30

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

rofi.opacity:  100
rofi.width:    90
rofi.lines:    25
rofi.location: 0
rofi.padding:  5
rofi.modi: combi,run,ssh
rofi.combi-modi: window,run,ssh
rofi.show: combi
rofi.color-enabled: true

URxvt.tabbed.no-tabbedex-keys: true
URxvt.tabbed.new-button: false
URxvt.keysym.Shift-Down: perl:tabbedex:new_tab
URxvt.keysym.Control-D: perl:tabbedex:close_tab
URxvt.keysym.Shift-Left: perl:tabbedex:prev_tab
URxvt.keysym.Shift-Right: perl:tabbedex:next_tab
URxvt.keysym.Shift-Up: perl:tabbedex:rename_tab
